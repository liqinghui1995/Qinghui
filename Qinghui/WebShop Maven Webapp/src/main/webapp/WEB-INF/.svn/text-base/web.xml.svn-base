<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>DiningServlet</servlet-name>
		<servlet-class>com.cblue.modual.user.action.DiningRoomServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.cblue.modual.user.action.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NourishmetFoodServlet</servlet-name>
		<servlet-class>com.cblue.modual.nourishmetfood.action.NourishmetFoodServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UsersServlet</servlet-name>
		<servlet-class>com.cblue.modual.user.action.UsersServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeViewPagerServlet</servlet-name>
		<servlet-class>com.cblue.modual.user.action.HomeViewPagerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeHotfood</servlet-name>
		<servlet-class>com.cblue.modual.user.action.HomeHotfood</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SearchCityServlet</servlet-name>
		<servlet-class>com.cblue.modual.user.action.SearchCityServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchCityServlet</servlet-name>
		<url-pattern>/searchcity</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeHotfood</servlet-name>
		<url-pattern>/homehotfood</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeViewPagerServlet</servlet-name>
		<url-pattern>/homeViewPagerServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UsersServlet</servlet-name>
		<url-pattern>/usersservlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>NourishmetFoodServlet</servlet-name>
		<url-pattern>/nourishment</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DiningServlet</servlet-name>
		<url-pattern>/dingroom</url-pattern>
	</servlet-mapping>
	
</web-app>
