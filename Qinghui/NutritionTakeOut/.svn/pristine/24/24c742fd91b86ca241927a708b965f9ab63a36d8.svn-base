<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_phlv_item_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_phlv_item_item3"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="sssss1"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/food_img"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tv_phlv_item_item"
       />

    <TextView
        android:id="@+id/tv_phlv_item_item3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/food_img"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/food_img"
        android:text="sssss2"
        android:textSize="16sp" />

</RelativeLayout>
